{
    "name": "benlumley/nova-impersonate-frontend",
    "description": "Frontend aspect of kabbouchi/nova-impersonate ripped out to work with Nova4's built in impersonate",
    "keywords": [
        "laravel",
        "nova",
        "impersonate"
    ],
    "license": "MIT",
    "require": {
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "BenLumley\\NovaImpersonateFrontend\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "BenLumley\\NovaImpersonateFrontend\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
